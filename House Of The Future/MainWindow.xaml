<Window x:Class="House_Of_The_Future.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:House_Of_The_Future"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="226,151,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="btnOn" Content="Turn ON" HorizontalAlignment="Left" Margin="99,205,0,0" VerticalAlignment="Top" Width="75" Click="btnOn_Click"/>
        <Button x:Name="btnStatus" Content="Check Status" HorizontalAlignment="Left" Margin="226,205,0,0" VerticalAlignment="Top" Width="75" Click="btnStatus_Click"/>
        <Button x:Name="btnOff" Content="Turn Off" HorizontalAlignment="Left" Margin="341,205,0,0" VerticalAlignment="Top" Width="75" Click="btnOff_Click"/>
        <Button x:Name="btnOn_Copy" Content="Turn ON" HorizontalAlignment="Left" Margin="99,230,0,0" VerticalAlignment="Top" Width="75" Click="btnOn_Copy_Click"/>
        <Button x:Name="btnStatus_Copy" Content="Check Status" HorizontalAlignment="Left" Margin="226,230,0,0" VerticalAlignment="Top" Width="75" Click="btnStatus_Copy_Click"/>
        <Button x:Name="btnOff_Copy" Content="Turn Off" HorizontalAlignment="Left" Margin="341,230,0,0" VerticalAlignment="Top" Width="75" Click="btnOff_Copy_Click"/>
        <Button x:Name="btnStatusses" Content="Inputs" HorizontalAlignment="Left" Margin="226,254,0,0" VerticalAlignment="Top" Width="75" Click="btnStatusses_Click"/>

    </Grid>
</Window>
