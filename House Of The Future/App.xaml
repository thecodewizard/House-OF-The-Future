<Application x:Class="House_Of_The_Future.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:House_Of_The_Future"
             xmlns:vm="clr-namespace:House_Of_The_Future.ViewModel"
             xmlns:helpers="clr-namespace:House_Of_The_Future.Helpers"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:MainViewModel x:Key="mainvm" x:Name="mainvm" />
            <helpers:LightImageConverter x:Key="lightImageConverter" x:Name="lightImageConverter" />
            <helpers:InverseBoolConverter x:Key="inverseBoolConverter" x:Name="inverseBoolConverter" />
            <helpers:BoolConverter x:Key="boolConverter" x:Name="boolConverter" />
            <helpers:ModeConverter x:Key="modeConverter" x:Name="modeConverter" />
            <helpers:PoortConverter x:Key="poortConverter" x:Name="poortConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
